<UserControl x:Class="JyGame.UserControls.DodgeMan"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="80" d:DesignWidth="80" >

    <Canvas>
        <Rectangle Width="80" Height="80" Fill="Red" x:Name="RedRect" RenderTransformOrigin="0.5,0.5" Stroke="#FFFF9A00">
            <Rectangle.RenderTransform >
                <TranslateTransform X=" 0" Y="0" x:Name="RedRectPoint"></TranslateTransform>
            </Rectangle.RenderTransform>
        </Rectangle>
    </Canvas>
</UserControl>
