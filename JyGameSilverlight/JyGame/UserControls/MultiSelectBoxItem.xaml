<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="JyGame.MultiSelectBoxItem" Width="400" Height="22" MouseEnter="UserControl_MouseEnter" MouseLeave="UserControl_MouseLeave" MouseLeftButtonUp="UserControl_MouseLeftButtonUp">

	<Grid x:Name="LayoutRoot">
		<TextBlock x:Name="content" TextWrapping="Wrap" Text="Content" Foreground="#E5FDDC00" FontWeight="Bold" FontSize="16" Margin="10,0,0,0">
			<TextBlock.Effect>
				<DropShadowEffect ShadowDepth="2" Opacity="0.69"/>
			</TextBlock.Effect>
		</TextBlock>
	</Grid>
</UserControl>