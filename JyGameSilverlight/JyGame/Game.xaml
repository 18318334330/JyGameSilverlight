<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:JyGame"
	mc:Ignorable="d"
	x:Class="JyGame.Game"
    Loaded="UserControl_Loaded"
    >

	<Canvas x:Name="RootCanvas" HorizontalAlignment="Center" VerticalAlignment="Center" Width="800" Height="600">
		<local:MainPage x:Name="mainPage" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top"/>
	</Canvas>
</UserControl>